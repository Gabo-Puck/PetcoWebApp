    <!-- Sweetalert -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <title>Lista usuarios reportados</title>
    <style>
      .openMenuMascotas {
        transform: rotate(270deg);
        display: inline-block;
      }
      .closeMenuMascotas {
        transform: rotate(180deg);
        display: inline-block;
      }

      .btnMenuMascota {
        background-color: transparent;
        border: none;
      }
      .btnMenuMascotaOpen {
        position: absolute;
      }
      .mascotaMenuOffCanvas {
        border: none;
        background-color: transparent;
        padding: 1em 0;
        width: 100%;
        border-top: 1px solid black;
        /* border-bottom: -1px solid black; */
      }

      .fotoMascota {
        background-size: 100% 100%;
        width: 8vh;
        height: 8vh;
        background-repeat: no-repeat;
      }

      #offcanvasBodyMascotas > *:last-child {
        border-bottom: 1px solid black;
      }
      h5,
      button {
        word-break: break-all;
      }
      button {
        text-align: center;
      }
      .activeButtonMascota {
        background-color: #89b3f1;
        color: white;
      }
      #offcanvasScrolling {
        background-color: #66a3ff;
      }
      .loadingScreenLayer {
        height: 100vh;
      }
      .mainContainerSolicitudes {
        height: 100vh;
      }
      .loadingSpinnerSolicitudes {
        z-index: 2000;
      }
      .fotoPerfilSolicitud {
        /* background-image: url("https://m.media-amazon.com/images/I/71ZcXHsUk1L.__AC_SX300_SY300_QL70_FMwebp_.jpg"); */
        /* background-size: 100% 100%; */
        width: 80%;
        text-align: center;
        position: relative;
        padding-bottom: 80%;
        border-radius: 50%;
        overflow: hidden;
        background-color: #3c8bfb;
      }
      .fotoPerfilSolicitud img {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
        object-fit: cover;
      }
      .controlSolicitud div {
        width: 50%;
      }
      .controlSolicitud div button {
        width: 100%;
      }

      @media (max-width: 767px) {
        .fotoPerfilSolicitud {
          width: 40%;
          padding-bottom: 40%;
        }
      }
      @media (min-width: 768px) and (max-width: 1023px) {
        .fotoPerfilSolicitud {
          width: 100%;
          padding-bottom: 100%;
        }
      }
      td{
   

      }
    </style>
    <script></script>
  </head>
  <body>
    <%- include ("../partials/header") %>

    <div class="preguntaTemplateFormularioSolicitud card d-none mb-3">
      <p class="preguntaFormularioSolicitud fs-4 fw-bold ms-2 mb-1"></p>
      <ul class="respuestasFormularioSolicitud"></ul>
    </div>
    <h1 class="text-center my-3">Usuarios reportados</h1>
    <div class="table-responsive px-3">
      <table class="table table-borderless table-striped table-hover align-middle">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Rep.</th>
            <th scope="col"># Reportes</th>
          </tr>
        </thead>
        <tbody class="table-group-divider">
          <%for(let i=0;i<UsuariosReportados.length;i++){%>
            <tr>
              <th scope="row"><%=i+1%></th>
              <td class="text-break"><%=UsuariosReportados[i].UsuarioRegistro.Nombre%></td>
              <td class="text-break"><%=UsuariosReportados[i].Reputacion%></td>
              <td class="text-break"><%=UsuariosReportados[i].numeroReportes%></td>
              <td class="text-break mx-auto align-center">
                <a href="/moderador/verUsuarioReportes/<%=UsuariosReportados[i].ID%>">
                  <button class="btn btn-info px-1">
                    <i class="fa-solid fa-arrow-up-right-from-square"></i>
                  </button> 
                </a>
              </td>
            </tr>
            <%}%>
        </tbody>
      </table>
    </div>
    <script type="module" src="/javascripts/feed.js"></script>
    <script>
      
    </script>
  </body>
</html>
